{
    "manifest_version": 3,
    "name": "YouTube Time Tracker",
    "version": "1.0",
    "description": "YouTubeページを開いている時間を記録します。",
    "permissions": [
      "tabs",
      "storage",
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  